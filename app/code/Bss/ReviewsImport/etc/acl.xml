<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::system">
                    <resource id="Magento_Backend::convert">
                        <resource id="Bss_ReviewsImport::importexport_product_review" title="Import/Export Product Reviews" translate="title">
                            <resource id="Bss_ReviewsImport::export_product_review" title="Export Product Reviews" translate="title"/>
                            <resource id="Bss_ReviewsImport::import_product_review" title="Import Product Reviews" translate="title"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
